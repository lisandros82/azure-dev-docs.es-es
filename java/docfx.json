{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/java/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "disable_contribution_on_translation_quality": ["MT"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/azure-dev-docs.es-es",
      "feedback_product_url": "https://github.com/Azure/azure-sdk-for-java/issues",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "Azure.AzureJava",
          "folder_relative_path_in_docset": "docs-ref-conceptual"
        }
      }
    },
    "fileMetadata": {
      "ms.devlang": {
        "**/*.md": "java"
      },
      "ms.service": {
        "**/*.md": "azure-java"
      },
      "manager": {
        "**/*.md": "barbkess"
      },
      "ms.author": {
        "**/*.md": "brendm"
      },
      "author": {
        "**/*.md": "bmitchell287"
      }
    },
    "template": [],
    "markdownEngineName": "markdig"
  }
}

